{
  "name": "container-storybook",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run webpack:serve",
    "build": "npm run webpack:build",
    "test": "npm run jest:test",
    "eslint:lint": "eslint 'src/**'",
    "eslint:fix": "eslint 'src/**' --fix",
    "ts:check": "tsc",
    "webpack:build": "webpack",
    "webpack:serve": "webpack-dev-server --inline --hot",
    "node:checkpath": "node -e \"var path = require('path'); console.log(path.resolve('/', './index.html'));\"",
    "jest:test": "jest --config 'jest.config.json' --no-cache",
    "jest:coverage": "jest --config 'jest.config.json' --no-cache --coverage",
    "storybook:serve": "start-storybook --port 6060",
    "storybook:build": "build-storybook --config-dir .storybook --output-dir storybook-static",
    "storybook:shot": "storycap http://localhost:6060 --viewport 1024x768 --viewport 320x568"
  },
  "scripts-info": {
    "start": "Entry point Start",
    "build": "Entry point Build",
    "webpack:serve": "Serve via webpack",
    "webpack:build": "Build via webpack",
    "eslint:lint": "Lint via EsLint",
    "eslint:fix": "Fix via EsLint",
    "ts:check": "Check via TypeScript",
    "node:checkpath": "Check path via NodeJs",
    "jest:test": "Testing via Jest",
    "jest:coverage": "Make coverage report via Jest",
    "storybook:serve": "Start server with Storybook",
    "storybook:build": "Build Storybook bundle",
    "storybook:shot": "Make screenshots via Storybook Storycap",
    "test": "Entry point Test"
  },
